{
  "name": "test-todo",
  "version": "1.0.0",
  "description": "## Prerequisite",
  "main": "index.js",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "lint": "eslint --ext .ts ./src --fix && eslint --ext .ts ./tests  --fix",
    "build": "rm -rf dist && tsc -p tsconfig-build.json",
    "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.test.ts --bail --exit --timeout 10000",
    "migrate": "sequelize db:migrate",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "db:create": "npx sequelize-cli db:create"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [],
  "author": "Navin Mishra",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.3",
    "helmet": "^6.1.5",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.6.0",
    "swagger-routes-express": "^3.3.2",
    "swagger-ui-express": "^4.6.2",
    "yaml": "^2.2.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
